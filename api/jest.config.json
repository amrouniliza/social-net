{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "./",
    "roots": [
        "<rootDir>"
    ],
    "modulePaths": [
        "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "./coverage"
  }
  